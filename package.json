{
  "name": "@ymgn-dev/zodify",
  "type": "module",
  "version": "2.5.0",
  "description": "TypeSpec to Zod schema generator",
  "author": "ymgn-dev",
  "license": "MIT",
  "homepage": "https://github.com/ymgn-dev/zodify#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymgn-dev/zodify.git"
  },
  "keywords": [
    "openapi",
    "zod",
    "schema",
    "generator"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "zodify": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "yaml": "^2.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.4.1",
    "@types/node": "^22.5.4",
    "eslint": "^9.10.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0"
  }
}
